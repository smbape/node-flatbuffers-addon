{
  "name": "flatbuffers-addon",
  "version": "1.0.3",
  "description": "Node flatbuffers addon",
  "repository": {
    "type": "git",
    "url": "https://github.com/smbape/node-flatbuffers-addon"
  },
  "keywords": [
    "flatbuffers"
  ],
  "engines": {
    "node": ">=8.0.0"
  },
  "main": "index.js",
