    "node-gyp": "^6.1.0",
    "request": "^2.88.2",
    "tar": "^6.0.1"
  },
  "devDependencies": {
    "benchmark": "^2.1.4",
    "chai": "^4.2.0",
    "flatbuffers": "^1.12.0",
    "mocha": "^7.1.0",
